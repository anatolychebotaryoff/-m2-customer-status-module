<config>
    <modules>
        <SM_SMOgone>
            <version>1.0.0</version>
        </SM_SMOgone>
    </modules>
    <global>
        <resources>
           <smogone_setup>
                <setup>
                    <module>SM_SMOgone</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </smogone_setup>
            <smogone_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </smogone_write>
            <smogone_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </smogone_read>
        </resources>
    </global>
</config>
